{
  "name": "maimai",
  "version": "1.0.0",
  "description": "A serverless fullstack framework for Node.js",
  "private": true,
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r --parallel run dev",
    "type-check": "pnpm -r run type-check",
    "test": "pnpm -r run test",
    "clean": "pnpm -r run clean",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish",
    "playground": "pnpm --filter @maimai/playground dev",
    "docs:dev": "pnpm --filter @maimai/docs dev",
    "docs:build": "pnpm --filter @maimai/docs build"
  },
  "keywords": [
    "serverless",
    "framework",
    "nodejs",
    "fullstack",
    "typescript"
  ],
  "author": "maimai",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "typescript": "^5.0.0",
    "prettier": "^3.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
