{
  "name": "@maimai/playground",
  "version": "0.1.0",
  "description": "Maimai framework development playground",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@maimai/core": "workspace:*",
