    "@maimai/decorators": "workspace:*",
    "@maimai/plugins": "workspace:*",
    "@maimai/types": "workspace:*",
    "@maimai/utils": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "tsup": "^8.0.0",
    "@types/node": "^20.0.0"
  }
}
