{
  "name": "@maimai/core",
  "version": "0.1.0",
  "description": "Maimai framework core SDK - 接口定义与装饰器",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "serverless",
    "framework",
    "nodejs",
    "fullstack",
    "ioc",
    "decorators",
    "typescript"
  ],
  "author": "maimai-js",
  "license": "MIT",
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsup": "^8.0.1"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maimai-js/maimai.git",
    "directory": "packages/core"
  }
}
